<Window x:Class="ComicsReadProgress.Management"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComicsReadProgress"
        FontSize="16"
        mc:Ignorable="d"
        Title="Management" Height="600" Width="600">
    <StackPanel Orientation="Horizontal" Margin="10">
        <ListBox ItemsSource="{Binding}" Name="ComicsList" Width="300"></ListBox>
        <StackPanel Margin="5,0,0,0">
            <TextBox Name="Year">2014</TextBox>
            <TextBox Name="Week">22</TextBox>
            <Button Click="LoadComicsClick">Загрузить комиксы</Button>
        </StackPanel>
    </StackPanel>
</Window>
