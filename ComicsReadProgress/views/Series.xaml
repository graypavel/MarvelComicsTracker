<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComicsReadProgress.views"
        xmlns:ComicsReadProgress="clr-namespace:ComicsReadProgress" x:Class="ComicsReadProgress.views.Series"
        FontSize="16"
        mc:Ignorable="d"
        Title="Series" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBox Name="SeriesTitle"></TextBox>
        <ListBox x:Name="SeriesList" ItemsSource="{Binding}" />

    </StackPanel>
</Window>
